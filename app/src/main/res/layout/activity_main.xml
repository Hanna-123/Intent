<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:layout_column="1"
>

    <TableRow>
        <TextView></TextView>
        <TextView
            android:layout_width="133dp"
            android:layout_column="0"
            android:layout_margin="20px"
            android:background="#92e99f"
            android:padding="15px"
            android:text="Ingresa un numero natural"
            android:textAlignment="center"
            android:textColor="#555"
            android:textSize="40px" />

    </TableRow>
    <TableRow>
        <TextView></TextView>
        <EditText
            android:layout_width="wrap_content"
            android:layout_margin="20px"
            android:hint="Numero Natural"
            android:id="@+id/xet1"/>
    </TableRow>
    <TableRow>

        <Button
            android:id="@+id/xbn1"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:text="1"
            android:textSize="50px"
            android:onClick="Clickxbn1"/>

        <Button android:layout_width="139dp"
            android:id="@+id/xbn2"
            android:text="2"
            android:textSize="50px"
            android:onClick="Clickxbn2"/>


        <Button android:layout_width="122dp"
            android:id="@+id/xbn3"
            android:text="3"
            android:textSize="50px"
            android:onClick="Clickxbn3"/>

    </TableRow>
    <TableRow>
        <Button
            android:layout_width="37dp"
            android:layout_height="wrap_content"
            android:id="@+id/xbn4"
            android:text="4"
            android:textSize="50px"
            android:onClick="Clickxbn4"/>

        <Button android:layout_width="139dp"
            android:id="@+id/xbn5"
            android:text="5"
            android:textSize="50px"
            android:onClick="Clickxbn5"/>

        <Button android:layout_width="122dp"
            android:id="@+id/xbn6"
            android:text="6"
            android:textSize="50px"
            android:onClick="Clickxbn6"/>

    </TableRow>
    <TableRow>
        <Button
            android:layout_width="37dp"
            android:layout_height="wrap_content"
            android:id="@+id/xbn7"
            android:text="7"
            android:textSize="50px"
            android:onClick="Clickxbn7"/>

        <Button android:layout_width="139dp"
            android:id="@+id/xbn8"
            android:text="8"
            android:textSize="50px"
            android:onClick="Clickxbn8"/>

        <Button android:layout_width="122dp"
            android:id="@+id/xbn9"
            android:text="9"
            android:textSize="50px"
            android:onClick="Clickxbn9"/>

    </TableRow>
    <TableRow>
        <Button
            android:layout_width="37dp"
            android:layout_height="wrap_content"
            android:id="@+id/xbn0"
            android:text="0"
            android:textSize="50px"
            android:onClick="Clickxbn0"/>

        <Button android:layout_width="139dp"
            android:text="Clear"
            android:id="@+id/xbnC"
            android:onClick="Clear"/>

        <Button android:layout_width="122dp"
            android:id="@+id/xbnR"
            android:text="="
            android:textSize="50px"/>

    </TableRow>

</TableLayout>
